{
  "name": "stat-reporter",
  "version": "0.0.1",
  "description": "Common plugin for gemini and hermione tools for displaying test browser statistic",
  "repository": {
    "type": "git",
    "url": "https://github.com/gemini-testing/stat-reporter.git"
  },
  "scripts": {
    "lint": "eslint .",
    "test": "npm run lint && npm run test-unit",
    "test-unit": "mocha test",
    "cover": "istanbul cover _mocha test",
    "precommit": "npm run lint",
    "commitmsg": "conventional-changelog-lint -e",
    "release": "standard-version"
  },
  "engines": {
    "node": ">= 4"
  },
  "keywords": [
    "gemini",
    "hermione",
    "plugin",
    "browser",
    "stat",
    "statistic",
    "report"
  ],
  "author": "bemer@yandex-team.ru",
  "license": "ISC",
  "dependencies": {
    "chalk": "^1.1.3",
    "cli-table": "^0.3.1",
    "fs-extra": "^1.0.0",
    "gemini-configparser": "^0.3.0",
    "html-tablify": "^2.0.2",
    "lodash": "^4.15.0",
    "moment": "^2.17.1"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "conventional-changelog-lint": "^1.0.1",
    "eslint": "^3.4.0",
    "eslint-config-gemini-testing": "^2.2.0",
    "husky": "^0.11.9",
    "istanbul": "^0.4.5",
    "mocha": "^3.0.2",
    "proxyquire": "^1.7.10",
    "qemitter": "^1.1.0",
    "sinon": "^1.17.5",
    "standard-version": "^3.0.0"
  }
}
